<script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");</script>
<script type="text/javascript" defer="defer">
var frontMaxWidth = 450;
var sideMaxWidth = 200;
var imageItemFrontWidth = 540;
var imageItemWidth = 360;
var prdVideoIndex = 0;
jQuery(document).ready(function(){
	
	// 페이스북 메타 테그 설정
	fbMeta();
	
	//앱스플레이
	var strmodelList = "IM-A830S,IM-A830K,IM-A830L";
	appsPlayList("30000000", strmodelList);
	
	// 따라다니는 제품 탭
	var smartTab = jQuery("#smartTab");
	//var fixedTop = 86; // 브라우저 스크롤이 minTop 이상일때 제품탭이 고정될 top 위치
	var fixedTop = 0; // 브라우저 스크롤이 minTop 이상일때 제품탭이 고정될 top 위치
	var headerTabTop = fixedTop + 110;
	var clickNum = -1;

	// 탭 이동
	jQuery("#smartTab li a").click(function(){
		var target = jQuery(jQuery(this).attr("href"));
		clickNum = parseInt(jQuery(this).attr("href").replace("#sTab", ""), 10);
		
		jQuery(smartTab).find("li").removeClass("on");
		jQuery(smartTab).find("li:eq(" + (clickNum - 1) + ")").addClass("on");
		
		jQuery("html:not(:animated), body:not(:animated)").animate({scrollTop : (jQuery(target).position().top - (jQuery(this).attr("href") == "#sTab2" ? 65 : 90))}, function(){
			clickNum = -1;
		});
		return false;
	});
	
	jQuery(window).scroll(function(){
		var minTop = jQuery("#sTab1").position().top - 104; // 브라우저의 scroll top 위치의 범위 설정
		var scrollTop = jQuery(window).scrollTop(); // 브라우저의 scroll top 위치
		//var sTab1PositionTop = jQuery("#sTab1").position().top;
		var sTab2PositionTop = jQuery("#sTab2").position().top;
		var sTab3PositionTop = jQuery("#sTab3").position().top;
		var sTab4PositionTop = jQuery("#sTab4").position().top;
		var sTab5PositionTop = jQuery("#sTab5").position().top;
		var sTab6PositionTop = jQuery("#sTab6").position().top;		
		
		
		/* debug("(scrollTop + headerTabTop)  : " + (scrollTop + headerTabTop));
		debugA("특징 : " + sTab1PositionTop + ", " + (sTab1PositionTop));
		debugA("스펙 : " + sTab2PositionTop + ", " + (sTab2PositionTop));
		debugA("이미지/동영상 : " + sTab3PositionTop + ", " + (sTab3PositionTop));
		debugA("리뷰 : " + sTab4PositionTop + ", " + (sTab4PositionTop));
		debugA("다운로드 : " + sTab5PositionTop + ", " + (sTab5PositionTop));
		debugA("앱&액세서리 : " + sTab6PositionTop + ", " + (sTab6PositionTop)); */
		
		if(scrollTop > minTop){
			jQuery(smartTab).css({'position' : 'fixed', 'top' : fixedTop + 'px', 'margin-top' : '0px'});
			
			var tabNo = 1;
			if((scrollTop + headerTabTop) < sTab2PositionTop){
				tabNo = 1;
			}else if((scrollTop + headerTabTop) < sTab3PositionTop){
				tabNo = 2;
			}else if((scrollTop + headerTabTop) < sTab4PositionTop){
				tabNo = 3;
			}else if((scrollTop + headerTabTop) < sTab5PositionTop){
				tabNo = 4;
			}else if((scrollTop + headerTabTop) < sTab6PositionTop){
				tabNo = 5;
			}else if((scrollTop + headerTabTop) < sTab6PositionTop){
				tabNo = 6;
			}else if((scrollTop + headerTabTop) < sTab6PositionTop){
				tabNo = 6;
			}else if($(window).scrollTop() == ($(document).height() - $(window).height())){
				tabNo = 6;
			}else{
				tabNo = 6;
			}

			if(clickNum > 0){
				tabNo = clickNum;
			}

			jQuery(smartTab).find("li").removeClass("on");
			jQuery(smartTab).find("li:eq(" + (tabNo - 1) + ")").addClass("on");
			
		}else{
			jQuery(smartTab).css({'position' : '', 'top' : '', 'margin-top' : ''});
		}
	});
	
	// 이미지 영역 리사이즈
	var imgListWidth = ($(".imgArea .imgList li").length *  imageItemWidth) + (imageItemFrontWidth - imageItemWidth) + 10;
	$(".imgArea .imgList").css({"width" : imgListWidth});
	
	$(".imgArea .imgList li img").each(function(i){
		$(this).load(function(){
			if(i == 0){
				resizeImage($(this), frontMaxWidth, false);
			}else{
				resizeImage($(this), sideMaxWidth, false);
			}			
		});
		if(i == 0){
			resizeImage($(this), frontMaxWidth, false);
		}else{
			resizeImage($(this), sideMaxWidth, false);
		}
	});
});

// 이미지 리사이즈
function resizeImage(imgObj, maxwidth, bAnimate){
	var maxWidth = maxwidth;
	var pImg = new Image();
	pImg.src = imgObj.attr("src");
	var imgWidth = pImg.width;
	var imgHeight = pImg.height;
	
	var newWidth = maxWidth;
	var ratio = imgWidth/maxWidth;
	var newHeight = imgHeight/ratio;
	
	if(bAnimate){
		imgObj.animate({
		    'height': newHeight + 'px',
		    'width': newWidth + 'px'
		});
	}else{
		imgObj.css({
			'height': newHeight + 'px',
		    'width': newWidth + 'px'
		});
	}
}
var imgdownload_page = 0;
var bImageMoving = false;
function imageSelect(imagePosition){
	var currentImagePosition = $(".imgArea .dotPaging a.on").index();
	
	if(! bImageMoving && currentImagePosition != imagePosition){
		bImageMoving = true;
		
		imgdownload_page = imagePosition;
		
		
		var selectItem = $(".imgArea .imgList li:eq("+ imagePosition +")");
		var currentItem = $(".imgArea .imgList li:eq("+ currentImagePosition +")");
		var positionDiff = currentImagePosition - imagePosition;

		$(selectItem).animate({
			'height' : '500px',
			'width' : '540px',
			'opacity' : '1',
			'margin-top' : 0
		});
		
		$(currentItem).animate({
			'height' : '400px',
			'width' : '360px',
			'opacity' : '0.5',
			'margin-top' : '100px'
		});
		
		positionLeft = imageItemWidth * (positionDiff);
		//proxied("positionLeft : " + positionLeft);
		$(".imgArea .imgList").animate({'left' : "+=" + positionLeft + 'px'}, function(){
			bImageMoving = false;
		});
		resizeImage(selectItem.find("img"), frontMaxWidth, true);
		resizeImage(currentItem.find("img"), sideMaxWidth, true);
		
		/* if(positionLeft < 0){
			resizeImage(selectItem.find("img"), frontMaxWidth, true);
			resizeImage(currentItem.find("img"), sideMaxWidth, true);
		}else{
			resizeImage(selectItem.find("img"), frontMaxWidth, true);
			resizeImage(currentItem.find("img"), sideMaxWidth, true);
		} */
		 $(".imgArea .dotPaging a").removeClass("on");
		 $(".imgArea .dotPaging a:eq(" +imagePosition + ")").addClass("on");
	}
}
jQuery(document).ready(function($){
	
	// 동영상 슬라이드 설정
	var bSlideMoving = false;
	var itemHeight = 154;
	
	function slideOuterHeight(selector){
		var sHeight = $(selector).find("li").length * itemHeight;
		$(selector).css("height",sHeight);
		return $(selector).outerHeight();
	}
	
	// 동영상 슬라이드
	$(".vList .slide ul").each(function(){
		slideOuterHeight($(this));
	});
	
	// 동영상 위/아래 버튼 숨김/표시
	if($(".vList .slide ul li").length > 2){
		$(".videoY .prev").show();
		$(".videoY .next").show();
	}else{
		$(".videoY .prev").addClass("off").find("img").attr({"src" : $(".videoY .prev").find("img").attr("src").replace("_on", "_off")});
		$(".videoY .next").addClass("off").find("img").attr({"src" : $(".videoY .next").find("img").attr("src").replace("_on", "_off")});
	}
	
	// 동영상 슬라이드 위로
	$(".videoY .prev").click(function(){
		var itemCnt = $(".vList .slide ul li").length;
		if (! bSlideMoving && itemCnt > 1 && ! $(this).hasClass("off")){
			$(".vList .slide ul li:last").prependTo($(".vList .slide ul"));
			$(".vList .slide ul").css({'top' : '-' + itemHeight + 'px'});
			
			bSlideMoving = true;
			$(".vList .slide ul").animate({'top' : '0px'}, function(){
				bSlideMoving = false;
			});
		}
		return false;
	});
	
	// 동영상 슬라이드 아래로
	$(".videoY .next").click(function(){
		var itemCnt = $(".vList .slide ul li").length;
		if (! bSlideMoving && itemCnt > 1 && ! $(this).hasClass("off")){
			bSlideMoving = true;
			
			$(".vList .slide ul").animate({'top' : '-' + itemHeight + 'px'}, function(){
				$(".vList .slide ul li:first").appendTo($(".vList .slide ul"));
				$(".vList .slide ul").css({'top' : '0px'});
				bSlideMoving = false;
			});
			
		}
		return false;
	});
	
	// 앱, 액세서리 슬라이드 설정
	var itemWidth = 960;
	
	function getPageText(pageNo){
		//return (pageNo < 10) ?  "0" + pageNo : pageNo;
		return pageNo;
	}
	
	function slideOuterWidth(selector){
		var sWidth = $(selector).find(".box").length * itemWidth;
		$(selector).css("width",sWidth);
		return $(selector).outerWidth();
	}
	// 리뷰 슬라이드 width 설정
	$(".reviewWrap .reviewSlide").each(function(){
        var sWidth = $(this).find(".reviewArea").length * itemWidth;
        $(this).css("width",sWidth);
    });

	// 다운로드 슬라이드 width 설정
	$(".downWrap .sSlide").each(function(){
		var sWidth = $(this).find(".downArea").length * itemWidth;
		$(this).css("width",sWidth);
	});
	
	// 앱, 액세서리 슬라이드 width 설정
	$(".slideInner").each(function(){
		slideOuterWidth($(this));
	});
	
	// 액세서리 좌/우 버튼 숨김/표시
	if($(".acceArea .slide .slideInner li").length > 8){
		$(".acceArea .sPaging").show();
	}else{
		$(".acceArea .sPaging").hide();
	}
	
	
	// 다운로드, 앱, 액세서리 슬라이드
	$(".sPaging").each(function(){
		var slide = $(this).parent().find(".slide").find(".slideInner");
		var page = $(this).find(".page>em");
		var aPage = $(this).find(".page>span.aPage");
		var itemCnt = $(slide).find(".box").length;
		if($(this).parents(".downWrap")){
			itemCnt = $(".downWrap .downArea").length;
		}
		
		$(aPage).text(getPageText(itemCnt));
		
		if(itemCnt === 0){
			page.text('0');
		}
		
		//페이징 넘버값 설정
		$("#sTab4 .reviewWrap .sPaging .page em").text("1");
        $("#sTab4 .reviewWrap .sPaging span.aPage").text($(".reviewWrap .reviewArea").length);

		$("#sTab5 .downWrap .sPaging .page em").text("1");
		$("#sTab5 .downWrap .sPaging span.aPage").text($(".downWrap .downArea").length);
		
		$("#sTab6 .prdAppsPlay .sPaging .page em").text("1");
		$("#sTab6 .prdAppsPlay .sPaging span.aPage").text($("#appslist ul").length);
		
		$("#sTab6 .acceArea .sPaging .page em").text("1");
		$("#sTab6 .acceArea .sPaging span.aPage").text($(".acceArea .slideInner ul").length);
		
		// 다운로드, 앱, 액세서리 슬라이드 이전
		$(".prev",this).click(function(){
			var pageNo = parseInt($(page).text(), 10);
			
			if($(this).parent().parent().is(".reviewWrap")){
                var slideDown = $(".reviewWrap .reviewSlide");
                // 리뷰 슬라이드
                var itemCnt1 = $(slideDown).find(".reviewArea").length;
                if (! bSlideMoving && itemCnt1 > 1){
                    $(slideDown).find(".reviewArea:last").prependTo($(slideDown));
                    $(slideDown).css({"left" : "-960px"});
                    
                    bSlideMoving = true;
                    $(slideDown).animate({"left" : "0px"}, function(){
                        bSlideMoving = false;
                    });
                    
                    $(page).text(getPageText(--pageNo));
                    
                    if(pageNo < 1){
                        pageNo = itemCnt1;
                        $(page).text(getPageText(pageNo));
                    }
                }
            }else if($(this).parent().parent().is(".downWrap")){
				var slideDown = $(".downWrap .sSlide");
				// 다운로드 슬라이드
				var itemCnt2 = $(slideDown).find(".downArea").length;
				if (! bSlideMoving && itemCnt2 > 1){
					$(slideDown).find(".downArea:last").prependTo($(slideDown));
					$(slideDown).css({"left" : "-960px"});
					
					bSlideMoving = true;
					$(slideDown).animate({"left" : "0px"}, function(){
						bSlideMoving = false;
					});
					
					$(page).text(getPageText(--pageNo));
					
					if(pageNo < 1){
						pageNo = itemCnt2;
						$(page).text(getPageText(pageNo));
					}
				}
			}else {
				//앱, 액세서리 슬라이드
				var itemCnt2 = $(slide).find(".box").length;
				if (! bSlideMoving && itemCnt2 > 1){
					$(slide).find(".box:last").prependTo($(slide));
					$(slide).css({"left" : "-" + itemWidth + "px"});
					
					bSlideMoving = true;
					$(slide).animate({"left" : "0px"}, function(){
						bSlideMoving = false;
					});
					
					$(page).text(getPageText(--pageNo));
					
					if(pageNo < 1){
						pageNo = itemCnt2;
						$(page).text(getPageText(pageNo));
					}
				}
			}
			
			return false;
		});
		
		// 다운로드, 앱, 액세서리 슬라이드 다음
		$(".next",this).click(function(){
			var pageNo = parseInt($(page).text(), 10);
			
			if($(this).parent().parent().is(".reviewWrap")){
                var slideDown = $(".reviewWrap .reviewSlide");
                // 다운로드 슬라이드
                var itemCnt2 = $(slideDown).find(".reviewArea").length;
                if (! bSlideMoving && itemCnt2 > 1){
                    bSlideMoving = true;
    
                    $(slideDown).animate({"left" : "-960px"}, function(){
                        $(slideDown).find(".reviewArea:first").appendTo($(slideDown));
                        $(slideDown).css({"left" : "0px"});
                        bSlideMoving = false; 
                    });
                    
                    $(page).text(getPageText(++pageNo));
                    
                    if(pageNo > itemCnt2){
                        pageNo = 1;
                        $(page).text(getPageText(pageNo));
                    }
                }
            }else if($(this).parent().parent().is(".downWrap")){
				var slideDown = $(".downWrap .sSlide");
				// 다운로드 슬라이드
				var itemCnt2 = $(slideDown).find(".downArea").length;
				if (! bSlideMoving && itemCnt2 > 1){
					bSlideMoving = true;
	
					$(slideDown).animate({"left" : "-960px"}, function(){
						$(slideDown).find(".downArea:first").appendTo($(slideDown));
						$(slideDown).css({"left" : "0px"});
						bSlideMoving = false; 
					});
					
					$(page).text(getPageText(++pageNo));
					
					if(pageNo > itemCnt2){
						pageNo = 1;
						$(page).text(getPageText(pageNo));
					}
				}
			}else{
				//앱, 액세서리 슬라이드
				var itemCnt2 = $(slide).find(".box").length;
				if (! bSlideMoving && itemCnt2 > 1){
					bSlideMoving = true;
	
					$(slide).animate({"left" : "-" + itemWidth + "px"}, function(){
						$(slide).find(".box:first").appendTo($(slide));
						$(slide).css({"left" : "0px"});
						bSlideMoving = false; 
					});
					
					$(page).text(getPageText(++pageNo));
					
					if(pageNo > itemCnt2){
						pageNo = 1;
						$(page).text(getPageText(pageNo));
					}
				}
			}
			return false;
		});
	});
	
	
	// 동영상 썸네일 클릭시
	$(".videoY .vList .slide ul li a").live("click", function(){
		var that = $(this);
		$(".topVideo .image").fadeOut(function(){
			$(".topVideo .video").hide();
			var thumbnail = $(that).find("img").attr("src");
			var videourl = $(that).attr("href");
			
			var videoPlayerArea = $('<div id="BBMCPlayer_video"/>');
			var videoImg = $("<img />");
			$(videoImg).attr({"src" : thumbnail});
			$(videoImg).attr({"alt" : $(that).find("img").attr("alt")});
			$(videoImg).attr({"title" : $(that).find("img").attr("title")});
			$(videoImg).css({"width" : "720px", "height" : "405px"});
			
			var playButton = $("<a></a>");
			$(playButton).addClass("play");
			$(playButton).append('<img src="/img/btn/btn_play02.png" alt="PLAY" title="PLAY"/>');
			$(playButton).attr({"href" : "javascript:viewMovie('" + videourl + "', '" + thumbnail + "')"});

			var closeButton = $("<a></a>");
			$(closeButton).addClass("close");
			$(closeButton).append('<img src="/img/btn/btn_close.png" alt="CLOSE" title="CLOSE"/>');
			$(closeButton).attr({"href" : "javascript:videoClose()"});
			
			videoPlayerArea.append(videoImg);
			videoPlayerArea.append(playButton);
			videoPlayerArea.append(closeButton);
			
			$(".topVideo .video").html('');
			$(".topVideo .video").append(videoPlayerArea);
			$(".topVideo .video").append('<a href="javascript:videoClose();" class="close"><img src="/img/btn/btn_close.png" alt="Close" /></a>');
			
			var btnHtml = '';
			btnHtml += '<div class="btn">';
			btnHtml += "<a href=\"javascript:viewMovie('" + videourl + "', '" + thumbnail + "', 'HD')\">";
			btnHtml += '<img src="/img/layout/btn_video_hd_off.gif" alt="HD" />';
			btnHtml += '</a>';
			btnHtml += '<a href="javascript:fn_share();">';
			btnHtml += '<img src="/img/layout/btn_video_share_off.gif" alt="Share" />';
			btnHtml += '</a>';
			btnHtml += '</div>';
			$(".topVideo .video").append(btnHtml);
			
			$(".topVideo .video").fadeIn();
		});
		prdVideoIndex = $(".videoY .vList .slide ul li a").index(that);
		return false;
	});
});

jQuery(function($){
	var on = function(){this.src = this.src.replace("_off","_on");};
	var off = function(){this.src = this.src.replace("_on","_off");};

	$(".specWrap .specBtn").click(function(){
		var normal = $(this).siblings(".normal");
		var detail = $(this).siblings(".detail");
		var easingOutType  = 'easeOutCirc';
		//var easingInType  = 'easeInCirc';
		var durationSpeed = 500;
		
		if($(this).hasClass("on")){
			$(this).removeClass("on").find("img").each(off);
			jQuery.blockUI();
			$(detail).hide();
			$(normal).hide();
			$(normal).slideDown({
				duration : durationSpeed,
				easing : easingOutType,
				complete : function(){
					jQuery.unblockUI();
				}
			});
			
			/* $(detail).slideUp({
				duration : durationSpeed,
				easing : easingOutType,
				complete : function(){
					$(normal).slideDown(durationSpeed, easingInType);
				}
			}); */
			
			//$(detail).slideUp(durationSpeed);
			//setTimeout(function(){$(normal).slideDown(durationSpeed)},durationSpeed);
		} else {
			$(this).addClass("on").find("img").each(on);
			jQuery.blockUI();
			$(normal).hide();
			$(detail).hide();
			$(detail).slideDown({
				duration : durationSpeed,
				easing : easingOutType,
				complete : function(){
					jQuery.unblockUI();
				}
			});
			/* $(normal).slideUp({
				duration : durationSpeed,
				easing : easingOutType,
				complete : function(){
					$(detail).slideDown(durationSpeed, easingInType);
				}
			}); */
			//$(normal).slideUp(durationSpeed);
			//setTimeout(function(){$(detail).slideDown(durationSpeed)},durationSpeed);
		}
		return false;
	});
	
	$(".downWrap").each(function(){
		var leng = $(".downArea",this).length;
		$(".sSlide",this).width(leng* 100 + "%");
	});
});

//파일다운로드
function fileDownload(){
	var src = jQuery(".viewImg"+imgdownload_page).attr("src");
	var srcSplit = src.split("/");
	var filename = srcSplit[srcSplit.length-1];
	location.href="/filedownload/download.do?fileName="+filename+"&flag=imv";
}

function viewContent(link){
	window.open(link);
}

function popAccView(lotsUrl){
	window.open(lotsUrl, "lots");
}

function viewMovie(url, thumb, qlt){
	if(_bMobile){
		location.href = getMovMobileUrl(url, qlt);
	}else{
		var width = "720";
		var height = "405";
		var auto = "true";
		$(".topVideo .video").html('<a href="javascript:videoClose();" class="close"><img src="/img/btn/btn_close.png" alt="Close" /></a><div id="BBMCPlayer_video"></div>');
		url = (qlt == 'HD' ? getMovHD(url) : url);
		getMovie(url, auto, width, height, thumb, "BBMCPlayer_video");
	}
}

// 배너 숨김
function bannerHide(){
	jQuery(".topVideo .bnr").hide();
}

//동영상 영역 닫기
function videoClose(){
	jQuery(".topVideo .video").fadeOut(function(){
		jQuery(".topVideo .image").fadeIn();
		jQuery(".topVideo .video").html('<div id="BBMCPlayer_video"></div>');
	});
}

//영상 완료시 호출하는 함수
function fn_movieComplete() {
	$(".videoY .vList .slide ul li a").eq(prdVideoIndex).trigger("click");
}

function clipboard(){
	
	var cp = $("xmp").html();
	
	var IE=(document.all)?true:false;
    if (IE) {
           window.clipboardData.setData("Text", cp);
        alert("주소가 복사 되었습니다.");
    } else {
        temp = prompt("Ctrl+C를 눌러 클립보드로 복사하세요", cp);
    }
}

//share 버튼 클릭시 호출하는 함수
function fn_share( url ) {
	fnShowLayerPop('vodShare');
}
</script>

