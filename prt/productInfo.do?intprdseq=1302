	FB.Event.subscribe('edge.create', function(href){
		fblike(href);
	});
 };
 (function(){
		var e = document.createElement('script');
		e.async = true;
		e.src = document.location.protocol + '//connect.facebook.net/ko_KR/all.js#appid=302278966514238&xfbml=1';
		document.getElementById('fb-root').appendChild(e);
	}());
 
 function fblike(likeUrl) {
	FB.api('/me', function(response) {
		FB.api({
			method: 'fql.query', 
			query: 'SELECT total_count FROM link_stat WHERE url="'+likeUrl+'"'
		   }, function(response) {});
	});
};

