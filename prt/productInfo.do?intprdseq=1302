                                <script type="text/javascript">
jQuery(document).ready(function(){
    // í¹ì§ ì´ë¯¸ì§    
    var new_fCnt = null;
    var new_fWidth = 960;
    var html = "";
    new_fCnt = jQuery("#featureView ul li").length;
    jQuery("#featureView ul").css({"width" : (new_fCnt * new_fWidth)});
    
    for(var i = 0; i < new_fCnt; i++){
        var href = jQuery("#featureView ul li:eq(" + i + ")").attr("id");
        html += '<a href="#' + href + '" class="' + (i == 0 ? 'on' : '') + '"><span>' + (i + 1) + '</span></a>';
    }
    jQuery("#featureViewPaging").html(html);


    // í¹ì§ ìì  2012-07-26     
    jQuery('#new_fView.new_slide ul li .fv_box4 div').css('display', 'block');
    vfb_click = 0;
    vfb_click0 = 0;
    vfb_click1 = 1;
    vfb_click2 = 2;
    vfb_click3 = 3;
    vfb_click4 = 4;
    vfb_click5 = 5;
    vfb_click6 = 6;
    vfb_click7 = 7;

    jQuery("#featureViewPaging a").live("click", function () {
        var featurePosition = jQuery(this).index();
        var currentFeaturetPosition = jQuery("#featureViewPaging a.on").index();
        var positionDiff = currentFeaturetPosition - featurePosition;
        var positionLeft = new_fWidth * positionDiff;

        jQuery("#featureView ul li:last").css({
            "left": new_fWidth * (new_fCnt - 1) + "px"
        });
        jQuery("#featureViewPaging a").removeClass("on");
        jQuery(this).addClass("on");

        jQuery("#featureView ul").animate({ 'left': "+=" + positionLeft + 'px' }, function ()
        {
            if (featurePosition == 0) {
                vfb_click = 10;
            } else if (featurePosition == 1) {
                if (vfb_click != 1 && vfb_click1 == 1) {
                    if (jQuery('.fv_box2 .izoom .p2').is(":not(:animated)")) {
                        vfb_click = 10;
                    }
                }
            } else if (featurePosition == 2) {
                if (vfb_click != 2 && vfb_click2 == 2) {
                    if (jQuery('.fv_box3').is(":not(:animated)")) {
                        vfb_click = 10;
                        if (zs_click == 1 && jQuery('.z_shutter dd').is(':animated') == false && jQuery('.z_shutter').is(':visible') == true) {
                        } else if (ts_click == 1 && jQuery('.t_focus p').is(':animated') == false && jQuery('.t_focus').is(':visible') == true) {
                        }
                    }
                }
            } else if (featurePosition == 3) {
                if (vfb_click != 3 && vfb_click3 == 3) {
                    if (jQuery('.fv_box4').is(":not(:animated)")) {
                        vfb_click = 30;
                        vfb_click3 = 30;
                    }
                }
            } else if (featurePosition == 4) {
                vfb_click = 10;
            } else if (featurePosition == 5) {
                if (vfb_click != 5 && vfb_click5 == 5) {
                    if (jQuery('.fv_box6').is(":not(:animated)")) {
                        vfb_click = 50;
                        vfb_click5 = 50;
                        jQuery('.fv_box6 p').css('display', 'none');
                    }
                }
            } else if (featurePosition == 6) {
                if (vfb_click != 6 && vfb_click6 == 6) {
                    if (jQuery('.fv_box7').is(":not(:animated)")) {
                        vfb_click = 60;
                        vfb_click6 = 60;
                    }                       
                }
            } else if (featurePosition == 7) {
                vfb_click = 10;
            };

        });
        
        return false;
    });
});
</script>

