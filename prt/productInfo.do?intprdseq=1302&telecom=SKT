            function fvb_play4() {
                function img_wifi() {
                    jQuery('.fv_box4 p.p1').fadeOut(200, function () {
                        jQuery('.fv_box4 p.p1').fadeIn(200);
                    });
                }

                function fv4_in() {
                    var fv4_w = jQuery('.fv4_white .fv4_w')
                    var fv4_b = jQuery('.fv4_black .fv4_w')
                    var fv4_p = (fv4_w.find('>p').width() * -1);

                    fv4_w.find('>p:first').clone().appendTo(fv4_w);
                    fv4_b.find('>p:first').clone().appendTo(fv4_b);

                    jQuery('.fv4_white .fv4_w').delay(500).animate({ left: fv4_p + 'px' }, 600, function () {
                        fv4_w.find('>p:first').remove();
                        fv4_w.css("left", "0");
                        img_wifi();

                        jQuery('.fv4_black .fv4_w').delay(300).animate({ left: fv4_p + 'px' }, 600, function () {
                            fv4_b.find('>p:first').remove();
                            fv4_b.css("left", "0");
                        });
                    });
                }

                function fv4_in_play() {
                    jQuery('.fv4_wbox').attr("timer", setInterval(function () { fv4_in() }, 2500));
                    setTimeout(function () {
                        clearInterval(jQuery('.fv4_wbox').attr("timer"));
                        vfb_click = 3;
                        vfb_click3 = 3;
                        return false;
                    }, 10000);
                    return false;
                }
                fv4_in();
                fv4_in_play();
            };

