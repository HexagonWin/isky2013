function appsPlayList(cate, model){
	jQuery("#sTab6 .appTabB li").each(function(){	jQuery(this).attr("class",""); });
	jQuery("#"+cate).attr("class","on");
	$.ajax({
		type: "GET",
		url: "/prt/getApps.do",
		timeout: 5000,
		data: {
			"category" : cate
			, "model" : model
			, "menu" : "IskyPopular"
		},
		async: false,
		dataType:"json",
		success: function(data){
			var html = "";
			switch(cate){
				case "10000000":
					catename = "game"
					break;
				case "30000000":
					catename = "movie"
					break;
				case "40000000":
					catename = "tv"
					break;
				case "50000000":
					catename = "music"
					break;
			}
			
			if(catename == "movie" || catename == "tv"){
				$("#sTab6 #prdAppsPlay").attr("class","prdAppsPlay tpB");
			}else{
				$("#sTab6 #prdAppsPlay").attr("class","prdAppsPlay");
			}
			
			if(data.result.code == 200)
			{
				for(var i=0; i<data.result.resultList.length;i++){
					var row =  data.result.resultList[i];
					if((i+1)%8 == 1){
						html += "<ul class='box'>";
					}
					var appTitleMaxLength = 20;
					var appTitle = (row.TITLE.length > appTitleMaxLength ? row.TITLE.substring(0, appTitleMaxLength) + "..." : row.TITLE); 
					html += '<li>'
					+'<a href="http://appsplay.vegalive.co.kr/'+catename+'/'+catename+'_view.sky?cid='+row.CID+'" target="_blank">'
					+'<span class="ico"><img src="'+row.IMAGE_ICON+'" alt="'+row.TITLE+'" /></span>'
					+'<span class="tit">'+appTitle+'</span>'
					+'</a>'
					+'<span class="star"><span style="width:'+(parseInt(row.RATE)*2)+'0%;"></span></span>'
					+'</li>';
					if((i+1)%8 == 0 || i == data.result.resultList.length-1){
						html += '</ul>';
					}
				}
				
				if(data.result.resultList.length == 0){
					html = "<ul class='box'><img src='/img/contents/smart/no_prd_image2.gif' alt='해당 데이터가 없습니다.' /></ul>";
				}
			}else{
				html = "<ul class='box'><img src='/img/contents/smart/no_prd_image2.gif' alt='해당 데이터가 없습니다.' /></ul>";
			}
			
			jQuery("#sTab6 #appslist").hide();
			jQuery("#sTab6 #appslist").html(html);
			//jQuery("#sTab6 #appslist ul li .ico").corner('cc:#fff round 15px');
			jQuery("#sTab6 #appslist").show();
			
			//슬라이드를 위한 설정
			$("#sTab6 #prdAppsPlay .sPaging .page em").text("1");
			$("#sTab6 #prdAppsPlay .sPaging span.aPage").text((parseInt(Math.ceil(data.result.resultList.length / 7), 10)));
			$("#sTab6 .slideInner").css("width", $("#sTab6 .slideInner").find(".box").length * 960);
		},
		error: function(e){
			var html = "<ul class='box'><img src='/img/contents/smart/no_prd_image2.gif' alt='해당 데이터가 없습니다.' /></ul>";
			jQuery("#appslist").html(html);
		}
	});
}
</script>
