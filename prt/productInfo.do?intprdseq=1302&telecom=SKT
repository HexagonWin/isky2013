            ts_click = 1;
            var tf_p = jQuery('.t_focus >p')
            function tf_set() {
                if (ts_click == 1) {
                    tf_p.fadeOut(1);
                    tf_p.eq(3).animate({ top: '-=12', left: '+=110' }, 300).animate({ left: '561px', top: '185px' }, 1)
                }
            }
            function t_focus() {
                if (ts_click == 1) {
                    ts_click = 0;
                    function tf_img() {
                        tf_p.eq(3).show().find('>img').animate({ width: '50px', top: '+=15', left: '+=15' }, 300, function () {
                            tf_p.eq(3).find('>img').animate({ width: '79px', top: '-=15', left: '-=15' }, 100)
                        });
                    }
                    tf_p.eq(0).delay(800).fadeIn(1000, function () {
                        tf_img();
                        tf_p.eq(0).delay(700).fadeOut(1500);
                        tf_p.eq(1).delay(700).fadeIn(1000);
                        tf_p.eq(3).delay(1300).animate({ top: '-=12', left: '+=110' }, 300).animate({ left: '-=13' }, 200, function () {
                            tf_img();
                            tf_p.eq(1).delay(700).fadeOut(1500);
                            tf_p.eq(2).delay(700).fadeIn(1000);
                            tf_p.eq(3).delay(1300).animate({ top: '+=19', left: '+=104' }, 300).animate({ left: '-=13' }, 200, function () {
                                tf_img();
                                ts_click = 1;
                            });
                        });

                    });
                }
            }
