                                        zs1_dd.eq(4).fadeIn(1000)
                                        zs2_dd.eq(4).delay(200).show(1).animate({ rotate: '30deg', translate: ['60px', '-153px'] }, 1000, function () {
                                            zs1_dd.eq(5).fadeIn(1000)
                                            zs2_dd.eq(5).delay(200).show(1).animate({ rotate: '40deg', translate: ['70px', '-183px'] }, 1000, function () {
