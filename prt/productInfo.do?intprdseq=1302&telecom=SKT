            function fvb_play2_set() {
                jQuery('.fv_box2 .izoom_rg').css({ top: '70px', left: '220px' })
                jQuery('.fv_box2 .izoom .p1').css({ top: '213px', left: '270px' })
            }
            function fvb_play2() {
                var iz_p = jQuery('.fv_box2 .izoom .p2')
                var iz_p1 = jQuery('.fv_box2 .izoom .p1')

                function izoom_p() {
                    iz_p.animate({ opacity: '1' }, 1, function () {
                        iz_p.fadeOut(500, function () { iz_p.fadeIn(500); })
                    })
                }
                function izoom_p1() {
                    iz_p1.hide(1).css({ top: '213px', left: '270px' })
                    iz_p1.show(1).animate({ top: '-=87px', left: '-=16px' }, 1000, null, function () {
                        iz_p1.delay(1000).hide(1)
                    });
                }


                if (iz_p.is(':animated')) {
                    return;
                } else {
                    iz_p.attr("timer", setInterval(function () { izoom_p() }, 500));
                    izoom_p1();

                }

                $(".fv_box2 .izoom").mousemove(
                function (e) {
                    var dimensions = $(".fv_box2 .zoom_s5").attr("rel").split(/\//),
                        offset = $(this).offset();
                    dimensions[0] = dimensions[0].split(/x/);
                    dimensions[1] = dimensions[1].split(/x/);
                    var ratio = {
                        width: parseInt(dimensions[0][1], 10) / parseInt(dimensions[1][1], 10),
                        height: parseInt(dimensions[0][0], 10) / parseInt(dimensions[1][0], 10)
                    },
                        x = e.pageX - offset.left,
                        y = e.pageY - offset.top,

                    x = x - $(this).width() / ratio["width"] / 2;
                    x1 = x - $(this).width() / 3 + 125;
                    y = y - $(this).height() / ratio["height"] / 2 - 15;

                    if (x1 < 0)
                        x1 = 0;
                    if (x1 > $(this).width() - $(this).width() / 3)
                        x1 = $(this).width() - $(this).width() / 3;

                    if (y < 0)
                        y = 0;
                    if (y > $(this).height() - $(this).height() / ratio["height"])
                        y = $(this).height() - $(this).height() / ratio["height"];

                    x1 = Math.round(x1);
                    x2 = Math.round(x1 - 60);
                    x3 = Math.round(x1 - 55);
                    y1 = Math.round(y);
                    y2 = Math.round(y - $(this).height());
                    y3 = Math.round(y - $(this).height() - 26);
                    $(this).find(".izoom_rg").css({
                        backgroundPosition: (x1 > 0 ? "-" + x1 + "px" : 0) + " " + (y1 > 0 - $(this).height() ? '-' + (y1 + $(this).height()) + "px" : 0), left: x1 + "px", top: y1 + "px"
                    });
                    $("div.zoom_s5").css("background-position", (x2 > 0 ? "-" + (x2 * ratio["width"] - 1) + "px" : 0) + " " + (y3 > 0 - $(this).height() ? '-' + ((y3 + $(this).height()) * ratio["width"] - 1) + "px" : 0));
                    $("div.zoom_b").css("background-position", (x3 > 0 ? "-" + (x3 * ratio["width"] - 1) + "px" : 0) + " " + (y3 > 0 - $(this).height() ? '-' + ((y3 + $(this).height()) * ratio["width"] - 1) + "px" : 0));
                    iz_p.stop().fadeOut(1)
                    iz_p1.stop().hide(1)
                    clearInterval(iz_p.attr("timer"));
                }
            );
            }
            // 제로 셔터 랙 // 트레킹 포커스
            // 제로 셔터 랙
