 function fblike(likeUrl) {
	FB.api('/me', function(response) {
		FB.api({
			method: 'fql.query', 
			query: 'SELECT total_count FROM link_stat WHERE url="'+likeUrl+'"'
		   }, function(response) {});
	});
};

