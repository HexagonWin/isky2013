                    zs1_dd.fadeOut()
                    jQuery('.zs_photo1 p3').fadeOut()
                    zs2_dd.animate({ rotate: '0deg', translate: ['0', '0px'], opacity: '0' }, 1)
                }
            }
            function z_shutter() {
                if (zs_click == 1 && navigator.appVersion.indexOf("MSIE 9") <= -1) {
                    zs_click = 0;
                    zs1_dd.eq(0).fadeIn(500);
                    zs2_dd.eq(0).animate({ rotate: '-30deg', opacity: '1' }, 500, function () {
                        zs1_dd.eq(1).fadeIn(500)
                        zs2_dd.eq(1).animate({ rotate: '-12deg', translate: ['-10px', '-38px'], opacity: '1' }, 500, function () {
                            zs1_dd.eq(2).fadeIn(500)
                            zs2_dd.eq(2).animate({ rotate: '10deg', translate: ['-3px', '-76px'], opacity: '1' }, 500, function () {
                                zs1_dd.eq(3).fadeIn(500)
                                zs2_dd.eq(3).animate({ rotate: '20deg', translate: ['30px', '-116px'], opacity: '1' }, 500, function () {
                                    zs1_dd.eq(4).fadeIn(500)
                                    zs2_dd.eq(4).animate({ rotate: '30deg', translate: ['60px', '-153px'], opacity: '1' }, 500, function () {
                                        zs1_dd.eq(5).fadeIn(500)
                                        zs2_dd.eq(5).animate({ rotate: '40deg', translate: ['70px', '-183px'], opacity: '1' }, 500, function () {
                                            zs_click = 1;
                                        });
                                    });
                                });
                            });
                        });
                    });
                } else if (zs_click == 1 && navigator.appVersion.indexOf("MSIE 9") > -1) {
                    zs2_dd.css({ filter: 'alpha(opacity=100)', opacity: '1', display: 'none' })
                    if (zs_click == 1) {
                        zs_click = 0;
                        zs1_dd.eq(0).fadeIn(1000);
                        zs2_dd.eq(0).delay(700).show(1).animate({ rotate: '-30deg' }, 700, function () {
                            zs1_dd.eq(1).fadeIn(1000)
                            zs2_dd.eq(1).delay(200).show(1).animate({ rotate: '-12deg', translate: ['-10px', '-38px'] }, 1000, function () {
                                zs1_dd.eq(2).fadeIn(1000)
                                zs2_dd.eq(2).delay(200).show(1).animate({ rotate: '10deg', translate: ['-3px', '-76px'] }, 1000, function () {
