                                phone_f.delay().find('>img').attr('src', phone_f.find('>img').attr('src').replace('front1.png', 'front2.jpg'))
                                phone_b.find('>p >img').css('width', '183px')
                                phone_b.delay(500).animate({ width: '213px', height: '418px', top: '75px', right: '31px' }, 1000)
                                phone_b.find('>p >img').delay(500).animate({ width: '213px', height: '418px' }, 1000, function () {
                                    jQuery('.t_icon2').fadeIn(1000);
                                    jQuery('.t_icon3').delay(500).fadeIn(1000, function () {
                                        vfb_click = 6;
                                        vfb_click6 = 6;
                                    });
                                });
                            });
                        });
                    });
                });
            }
            // 스마트 빔 set
            function fvb_play7_bs() {
                var phone_f = jQuery('.fv_box7 .phone_ft1')
                var phone_b = jQuery('.fv_box7 .phone_bk1')
                phone_f.find('>img').attr('src', phone_f.find('>img').attr('src').replace('front2.jpg', 'front1.jpg'))
                phone_f.css('display', 'none');
                phone_f.find('p').fadeOut(1);
                phone_f.find('p.view1').fadeIn(1);
                phone_f.css({ left: '480px', top: '100px' })
                phone_b.css('display', 'none');
                phone_b.css({ right: '0', top: '90px', width: '162px', height: '364px' })
                phone_b.find('p img').css({ width: '162px', height: '364px' })
                phone_b.find('p').animate({ reflectY: false }, 1)
                phone_b.find('>p >img').attr('src', phone_b.find('>p >img').attr('src').replace('back3', 'back1'))
                jQuery('.t_icon2,.t_icon3 ,.t_icon , .t_icon .p1, .t_icon .p2, .t_icon .p3').css('display', 'none');
            };

            
            if ("1302"  == '902') {//IM-A840 모델
                if (new_fCnt > 0) {
                
	                var featurePosition = jQuery(this).index();                                     
	                var leftP =  new_fWidth * featurePosition
	
