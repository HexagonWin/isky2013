<script type="text/javascript">
function Compare(n) {
	if(n==1) {
		document.all.m1.src="http://www.isky.co.kr/source/img/global/popup/compare_tab01_.gif";
		document.all.m2.src="http://www.isky.co.kr/source/img/global/popup/compare_tab02.gif";
		document.all.Compare1.style.display="block";
		document.all.Compare2.style.display="none";
	} else {
		document.all.m1.src="http://www.isky.co.kr/source/img/global/popup/compare_tab01.gif";
		document.all.m2.src="http://www.isky.co.kr/source/img/global/popup/compare_tab02_.gif";
		document.all.Compare1.style.display="none";
		document.all.Compare2.style.display="block";
	}
}
</script>
