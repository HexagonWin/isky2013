<script>
var oldnum;
oldnum = 1;
function View(num){
if (moz) { disview = 'table-row'; }
else if (ie) { disview = 'block'; }
	if((oldnum != 0 && oldnum != num)){
		eval("document.all.Layer"+oldnum+".style.display='none'");
		eval("document.all.Layer"+oldnum+".className='faqlno'");
		eval("document.all.Layer"+num+".style.display='"+disview+"'");
		eval("document.all.Layer"+num+".className='p_t5 p_b5'");
		oldnum = num;
	}else if(oldnum == num){
		eval("document.all.Layer"+oldnum+".style.display='none'");
		eval("document.all.Layer"+oldnum+".className='faqlno'");
		oldnum = 0;
	}else if(oldnum == 0){
		eval("document.all.Layer"+num+".style.display='"+disview+"'");
		eval("document.all.Layer"+num+".className='faql em_bg'");
		oldnum = num;
	}
}
</script>
