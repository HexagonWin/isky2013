	function goList(form,flag,flag1){
		if(flag1 == "1") {
			form.action = "skyevent.jsp";
		} else {
			form.action = "skyevent_prize.jsp";
		}
		form.flag.value = flag;
		form.submit();
	}
	function goView(form, eventid){
		form.action = "skyevent_view.jsp";
		form.eventid.value = eventid;
		form.submit();
	}
	function goIns(form, eventid){
		form.action = "skyevent_proc.jsp";
		form.eventid.value = eventid;
		form.submit();
	}
	function goIns1(form){
		form.action = "member_join.jsp";
		form.submit();
	}
	function goIns2(form){
		form.action = "openevent2_1.jsp";
		form.submit();
	}
	function goPrize(form, eventid,flag,flag1){
		form.action = "skyevent_prize_view.jsp";
		form.eventid.value = eventid;
		form.flag.value = flag;
		form.flag1.value = flag1;
		form.submit();
	}
	function goView2(form, eventid,flag,flag1, action){
	
		goLogin();
	
	}
