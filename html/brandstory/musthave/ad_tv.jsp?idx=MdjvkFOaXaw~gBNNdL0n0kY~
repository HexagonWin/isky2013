	function goView(form, idx){
		form.idx.value = idx;
		form.tv_state.value = "VOD";
		form.target = "_self";
		form.action = "ad_tv.jsp";
		form.submit();
	}

	function goMakingView(form, idx){
		form.idx.value = idx;
		form.tv_state.value = "MAK";
		form.target = "_self";
		form.action = "ad_tv.jsp";
		form.submit();
	}
