    function phone_regi(model_n) { 
        if(confirm(model_n+" 소지자만 다운로드 받으실수 있습니다. 제품등록 페이지로 이동하시겠습니까?"))
        {
           location.href = "/html/mysky/modify_product.jsp";
        }else{
           return;
        }
    }
    /** 미로그인 **/
    function go_login()
    {
        
            if(confirm("로그인 후 다운로드를 받을 수 있습니다.\n로그인 하시겠습니까?"))
            {
                goLogin();
            }
        
    }
