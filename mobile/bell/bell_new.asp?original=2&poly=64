<script language="javascript">
	function bell_pop01(param) {		
	var popup	= window.open("./bell_pop01.asp?bell_code="+param+"&poly=64", "frenp","height=200,width=400,toolbar=no,menubar=no,location=no,scrollbars=no,resizable=no,top=0,left=0");
	popup.focus();
	}

	function bell_pop02(param) {		
	var popup	= window.open("./bell_pop02.asp?bell_code="+ param+"&poly=64","frenp01","height=700,width=400,toolbar=no,menubar=no,location=no,scrollbars=no,resizable=no,top=0,left=0");
	popup.focus();
	}

	function YesNo(code) 	{
		if (confirm("마이찜에 등록 하시겠습니까?")) {
			document.frames['zzim'].location.href = "./bell_zzim.asp?code=" + code;
		}
	}
	
	function changeForm(obj) {
		location.href="./bell_list.asp?sort=" + obj.value
	}

		
</script>
