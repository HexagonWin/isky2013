<script language="javascript">
	function char_pop01(param) {		
		var popup	= window.open("./char_pop01.asp?event=Y&"+"character_code="+param+"&size=m", "frenp","height=700,width=380,toolbar=no,menubar=no,location=no,scrollbars=no,resizable=no,top=0,left=0");
		popup.focus();
	}

	function YesNo(code) 	{
		if (confirm("마이찜에 등록 하시겠습니까?")) {
			document.frames['zzim'].location.href = "./character_zzim.asp?code=" + code;
		}
	}	
</script>

<script language="JavaScript">
function resize_iframes() {
 if(document.body.scrollHeight > 0) {
  self.resizeTo(document.body.scrollWidth, document.body.scrollHeight);
 }
}
</script>
