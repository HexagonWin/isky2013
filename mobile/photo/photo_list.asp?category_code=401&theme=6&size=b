<script language="javascript">
	function photo_pop01(param) {					
		var popup	= window.open("/mobile/photo/photo_pop01.asp?photo_code="+param+"&size=b", "frenp","height=680,width=400,toolbar=no,menubar=no,location=no,scrollbars=no,resizable=no,top=0,left=0");
		popup.focus();
	}

	function YesNo(code) 	{
		if (confirm("마이찜에 등록 하시겠습니까?")) {
			document.frames['zzim'].location.href = "/mobile/photo/photo_zzim.asp?code=" + code;
		}
	}	
</script>
