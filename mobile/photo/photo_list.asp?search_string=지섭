	function photo_pop01(param) {					
		var popup	= window.open("/mobile/photo/photo_pop01.asp?photo_code="+param+"&size=m", "frenp","height=680,width=400,toolbar=no,menubar=no,location=no,scrollbars=no,resizable=no,top=0,left=0");
		popup.focus();
	}

	function YesNo(code) 	{
		if (confirm("¸¶ÀÌÂò¿¡ µî·Ï ÇÏ½Ã°Ú½À´Ï±î?")) {
			document.frames['zzim'].location.href = "/mobile/photo/photo_zzim.asp?code=" + code;
		}
	}	
