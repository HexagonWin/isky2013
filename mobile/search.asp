<script language="javascript">
	function all_search() {
	
		var forms=document.regfrom;
		if(forms.search_string.value==""){
			alert("검색어를 넣어 주세요.!");
				forms.search_string.focus();
				return;
			}	
		if(forms.sub_cate.value=="all"){
		forms.action = "search.asp";
		}
		else if(forms.sub_cate.value=="bell"){
		forms.action = "/mobile/bell/bell_list.asp";
		}
		else if(forms.sub_cate.value=="character"){
		forms.action = "/mobile/character/char_list.asp";
		}
		else if(forms.sub_cate.value=="photo"){
		forms.action = "/mobile/photo/photo_list.asp";
		}
		else if(forms.sub_cate.value=="sms"){
		forms.keyword.value = forms.search_string.value;
		forms.action = "/mobile/sms/sms_list.asp";
		}
		forms.submit();
	}
	function enter_check()
	{
	if(event.keyCode==13)
	{
		event.keyCode = 0;
	        all_search();
	                
	}
}

