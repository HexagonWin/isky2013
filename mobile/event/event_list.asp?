<script language="javascript">
	function all_search() {
	
		var forms=document.regfrom;
		if(forms.search_string.value==""){
			alert("검색어를 넣어 주세요.!");
				forms.search_string.focus();
				return;
			}	
		if(forms.sub_cate.value=="all"){
		forms.action = "search.asp";
		}
		else if(forms.sub_cate.value=="bell"){
		forms.action = "/mobile/bell/bell_list.asp";
		}
		else if(forms.sub_cate.value=="character"){
		forms.action = "/mobile/character/char_list.asp";
		}
		else if(forms.sub_cate.value=="photo"){
		forms.action = "/mobile/photo/photo_list.asp";
		}
		else if(forms.sub_cate.value=="sms"){
		forms.keyword.value = forms.search_string.value;
		forms.action = "/mobile/sms/sms_list.asp";
		}
		forms.submit();
	}
	function enter_check()
	{
	if(event.keyCode==13)
	{
		event.keyCode = 0;
	        all_search();
	                
	}
}
	function char_pop01(param) {
	var popup=window.open("/mobile/character/char_pop01.asp?character_code="+param+"&size=s", "frenp","height=700,width=380,toolbar=no,menubar=no,location=no,scrollbars=no,resizable=no,top=0,left=0");
	popup.focus();
	}

	function YesNo_char(code) 	{
		if (confirm("마이찜에 등록 하시겠습니까?")) {
			document.frames['zzim'].location.href = "/mobile/character/character_zzim.asp?code=" + code;
		}
	}	
	function photo_pop01(param) {
	var popup=window.open("/mobile/photo/photo_pop01.asp?photo_code="+param+"&size=s", "frenp","height=700,width=380,toolbar=no,menubar=no,location=no,scrollbars=no,resizable=no,top=0,left=0");
	popup.focus();
	}

	function YesNo_photo(code) 	{
		if (confirm("마이찜에 등록 하시겠습니까?")) {
			document.frames['zzim'].location.href = "/mobile/photo/photo_zzim.asp?code=" + code;
		}
	}
	function bell_pop01(param) {
		var popup=window.open("/mobile/bell/bell_pop01.asp?bell_code="+param+"&poly=64", "frenp","height=200,width=380,toolbar=no,menubar=no,location=no,scrollbars=no,resizable=no,top=0,left=0");
		popup.focus();
	}

	function bell_pop02(param) {
		var popup=window.open("/mobile/bell/bell_pop02.asp?bell_code="+ param+"&poly=64","frenp","height=700,width=380,toolbar=no,menubar=no,location=no,scrollbars=no,resizable=no,top=0,left=0");
		popup.focus();
	}

	function YesNo(code) 	{
		if (confirm("마이찜에 등록 하시겠습니까?")) {
			document.frames['zzim'].location.href = "/mobile/bell/bell_zzim.asp?code=" + code;
		}
	}	
</script>
