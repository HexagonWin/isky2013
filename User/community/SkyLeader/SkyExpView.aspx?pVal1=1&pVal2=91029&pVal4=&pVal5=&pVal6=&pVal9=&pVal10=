		//답변 가능 여부 체크 및 답변 등록 페이지로 이동
		function goReply()
		{
			var TopFg = "N";
			var StateFg = "N";
			
			if(TopFg == "Y")
			{
				alert("공지글에는 답변글을 등록하실 수 없습니다.");				
			}
			else if(StateFg == "M")
			{
				alert("다른 게시판으로 이동된 글에는\n답변글을 등록하실 수 없습니다.");
			}
			else
			{
				location.href = "SkyAzitExpWrite.aspx?pVal1=1"
									+ "&pVal3=91029&pVal4=&pVal9=&pVal10=&skyleader=";
			}
		}
		
