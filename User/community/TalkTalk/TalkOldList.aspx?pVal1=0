									+"&pVal5=&pVal6=&pVal9=&pVal10=";
			}
			
			//검색 조건 유효성 검사
			function goSearch()
			{
				var hiddenKeyword = document.all.hidKeyWord;
				var strKeyWord = document.all.txtKeyWord;
				if(strKeyWord.value == '키워드를 입력해 주세요' || strKeyWord.value == '')
				{
					alert('키워드를 입력해 주세요');
					strKeyWord.focus();
					return false;
				}
				else
				{
					hiddenKeyword.value = strKeyWord.value;
				}
			}
			
			//판매 구분으로 검색 시
			function goClass2Search(value)
			{
				location.href = "TalkOldList.aspx?pVal1=0&pVal4=&pVal5="
									+ "&pVal6=" + escape(value) + "&pVal9=&pVal10=";
			}
			
