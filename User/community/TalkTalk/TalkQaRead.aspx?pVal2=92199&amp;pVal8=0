			//답변 등록으로 이동 및 유효성 체크,
			function goReply()
			{
				var TopFg = "N";
				var StateFg = "N";
				
				if(TopFg == "Y")
				{
					alert("공지글에는 답변글을 등록하실 수 없습니다.");				
				}
				else if(StateFg == "M")
				{
					alert("다른 게시판으로 이동된 글에는\n답변글을 등록하실 수 없습니다.");
				}
				else
				{
					location.href = "TalkQaWrite.aspx?pVal1=0"
										+ "&pVal3=92199&pVal4=&pVal5=&pVal9=&pVal10=";
				}
			}
			
