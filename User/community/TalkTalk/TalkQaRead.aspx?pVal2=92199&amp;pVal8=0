			function RecdConfirm()
			{
				var TopFg = "N";
				if(TopFg == "Y")
				{
					alert("공지글에는 추천을 하실 수 없습니다.");	
					return false;		
				}
			}
			
			function PermConfirm()
			{
				if(confirm("해당 답변을 채택하시겠습니까?") == 1) 
					return true;
				else
					return false;		
			}
				
			var PopupWin = null;
			
			//게시물 인쇄 팝업
			function goPrint()
			{
				if(PopupWin != null && !PopupWin.closed) PopupWin.close();
				var src='../WUC/Print.aspx?MsgID=92199';
				PopupWin = window.open(src,'','toolbar=no,location=no,status=no,scrollbars=yes,width=600,height=100,resizable=no');
				PopupWin.window.focus();
