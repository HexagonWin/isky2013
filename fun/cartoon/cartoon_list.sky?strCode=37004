	function goList(targetPage, targetGroup) {
	
		var obj = $('cartoon');
	
		obj.targetPage.value = targetPage;
		obj.targetGroup.value = targetGroup;
		obj.action = "/fun/cartoon/cartoon_list.sky";
		obj.submit();
	}

	function CartoonView(intSeq){
		location.href = "/fun/cartoon/cartoon_view.sky?dispNum=&strCode=37004&intSeq=" + intSeq;
	}

</script>
