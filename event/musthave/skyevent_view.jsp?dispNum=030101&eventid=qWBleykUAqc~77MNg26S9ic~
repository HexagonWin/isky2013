<script language=javascript>
	function goList(form,flag,flag1){
		if(flag1 == "1") {
			form.action = "skyevent.jsp?dispNum=030101";
		} else {
			form.action = "eventResult.jsp?dispNum=030101";
		}
		form.flag.value = flag;
		form.submit();
	}
	function goView(form, eventid){
		form.action = "skyevent_view.jsp?dispNum=030101";
		form.eventid.value = eventid;
		form.submit();
	}
	function goPrize(form, eventid,flag,flag1){
		form.action = "skyevent_prize_view.jsp?dispNum=030101";
		form.eventid.value = eventid;
		form.flag.value = flag;
		form.flag1.value = flag1;
		form.submit();
	}
	function goView2(form, eventid,flag,flag1, action){
	
		goLogin();
	
