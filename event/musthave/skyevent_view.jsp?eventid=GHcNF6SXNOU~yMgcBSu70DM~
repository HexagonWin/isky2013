<script language="JavaScript">
var request = getRequest();

/*2007.12.28 스타폰2007경매 종료팝업*/


	function eventJoin() {
		var url = "/event/Event_080611/gallaEventProc.jsp?eventid2=" + document.getElementById("eventidReq").value;
		sendRequest(url, eventHandler);
	}

	String.prototype.trim = function()
	{//공백 제거 
		return this.replace(/(^[ \f\n\r\t]*)|([ \f\n\r\t]*$)/g, "");
	}
	
	function eventHandler() {
		if(request.readyState == 4) {
		//alert(request.status);
			if(request.status == 200) {			
				var result = request.responseText.trim();
				//alert(result);
				if(result == "success") {
					alert("이벤트에 성공적으로 응모되셨습니다.");
				} else if(result == "nouser") {
					alert("로그인 후 이벤트에 참여 하실 수 있습니다..");
					window.open("/member/loginPopup.jsp?gotype=7&p_url=/event/musthave/skyevent_view.jsp?eventid=GHcNF6SXNOU~yMgcBSu70DM~", "loginPopup", "width=340, height=280, left=0, top=0");
				} else if(result == "already") {
					alert("이미 이벤트에 참여하셨습니다.");
				} else if(result == "fail") {
					alert("시스템 오류입니다.\n관리자에게 문의해주세요.");
				} else{
					alert("잘못된 응답입니다.\n관리자에게 문의해주세요.");
				}
			}
		}
	}


</script>

