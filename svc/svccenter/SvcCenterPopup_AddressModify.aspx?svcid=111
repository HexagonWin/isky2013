		<script language="javascript">
		
		function ZipcodeReturn(value1,value2,value3,value4){
			
			lblNewAddr.innerHTML = value1+" "+value2+" "+value3+" "+value4;
			document.all["txtNewZip"].value = value1;
			document.all["txtNewAddr"].value = value2+" "+value3+" "+value4;
			document.all["txtNewAddr2"].focus();
			
		}
		</script>
		<script language="javascript">
					
			function SearchValidCheck()
			{
				var SearchVal = document.all['txtNewZip'].value;
			
				if(("" == SearchVal) || ("변경할 주소를 검색후 선택해 주세요" == SearchVal))
				{
					alert("변경할 주소를 검색후 선택해 주세요.");
					return false;
				}
				
				SearchVal = document.all['txtNewAddr2'].value;
			
				if(("" == SearchVal) || ("상세주소를 입력해 주세요." == SearchVal))
				{
					alert("상세주소를 입력해 주세요");
					document.all['txtNewAddr2'].focus();
					return false;
				}
				
				return true;
			}
			
		</script>
		<script language="javascript">
					
			function SearchValidCheck_Dong()
			{
				var SearchVal = document.all['txt_SearchDong'].value;
			
				if(("" == SearchVal) || ("읍/면/동 이름을 입력하세요." == SearchVal))
				{
					alert("읍/면/동 이름을 입력하세요.");
					document.all['txt_SearchDong'].focus();
					return false;
				}
				
				return true;
			}
			
		</script>
