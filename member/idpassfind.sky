	function resizeHeight() {

		if(window.JSON){
			
			var obj = document.getElementById('ifr_body');

			resizeFrame(obj);

		}else{
			fr = document.getElementById('ifr_body');

			fr.setExpression('height',ifr_body.document.body.scrollHeight+100);  
		}

  }

  function resizeFrame(iframeObj){
		var innerBody = iframeObj.contentWindow.document.body;

		var innerHeight = innerBody.scrollHeight + (innerBody.offsetHeight - innerBody.clientHeight);
		var innerWidth = innerBody.scrollWidth + (innerBody.offsetWidth - innerBody.clientWidth);
		
		if(window.JSON){
			innerHeight+=15;
		}

		iframeObj.style.height = innerHeight + "px";
		iframeObj.style.width = innerWidth + "px";
	}
</script>
