<script type="text/javascript">

//최신뉴스 사용
function goList(page){
	var parameter = jQuery("form[name='frm1']").serialize()+"&page="+page;
	jQuery("#bbsList").block();
	$.ajax({
		type: "POST",
		url: "ajaxViewList.do",
		data: parameter , 
		async: false,
		success: function(txt){
			
			jQuery.unblockUI();
			jQuery("#bbsList").html(txt);
		}
	});
}

function check_search(gb){
	goList(1);
	if(gb!="btn"){
		return false;
	}
}

jQuery(function(){
	goList("");
});
</script>
