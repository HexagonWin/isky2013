	function goScrapSource(){
		var str_embed_script = "<iframe src='http://www.isky.co.kr/source/fla/brand/player.swf?videoURL=http://dmfile.isky.co.kr/upload/musthave/tv/s240k_tv_cf.flv' name='MovieFla' width=270 height=202 ></iframe>";
