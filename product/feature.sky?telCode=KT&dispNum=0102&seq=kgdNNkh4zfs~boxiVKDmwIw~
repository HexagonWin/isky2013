	var webServerUrl = "http://www.isky.co.kr";
	var seq = "kgdNNkh4zfs~boxiVKDmwIw~"
	function featureCopy(){
		var obj = $('feature_view');

		var html = obj.innerHTML;
		var url = webServerUrl + "/images";
		
		html = html.replace(/\/images/gi, url);

		window.clipboardData.setData("Text",html);

		alert("클립보드에 복사되었습니다.");
		
	}

