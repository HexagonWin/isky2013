<script language="javascript">
	function goList(targetPage, targetGroup) {

		var obj = $('productList');;

		obj.targetPage.value = targetPage;
		obj.targetGroup.value = targetGroup;
		obj.action = "/product/searchResultList.sky";
		obj.submit();
	}

	function checkCompare() {

		var obj = document.productList;
		var checkedCnt = 0;

		for (var i = 0; i < obj.compareSeq.length; i ++) {
			if (obj.compareSeq[i].checked == true)
				checkedCnt ++;
		}

		if (checkedCnt == 0) {
			alert("비교할 제품을 선택해 주세요.");
			return false;
		} else {
			obj.target = "_parent";
			obj.action = "productCompare.sky";
			obj.submit();
		}
	}

	function limitCheckedCnt(val) {

		var obj = document.productList;
		var checkedCnt = 0;

		for (var i = 0; i < obj.compareSeq.length; i ++) {
			if (obj.compareSeq[i].checked == true)
				checkedCnt ++;
		}

		if (checkedCnt > 3) {
			alert("최대 3개 까지 선택하실 수 있습니다.");
			val.checked = false;
			return false;
		}
	}

	function goProductList() {

		var obj = document.productCompare;
		obj.target = "_parent";
		obj.action = "product.sky";
		obj.submit();
	}

	function Detail(dispNum, intPrdSeq){

		parent.location.href = "/product/feature.sky?dispNum=" + dispNum + "&seq=" + intPrdSeq;
	}
</script>
