	}

	function goList(targetPage, targetGroup) {

		var obj = document.reviewPage;

		obj.targetPage.value = targetPage;
		obj.targetGroup.value = targetGroup;
		obj.action = "/product/review.sky";
		obj.submit();
	}

	function detail(intReviewSeq){
		
		var targetPage = $('targetPage');
		var targetGroup = $('targetGroup');

		var url = "/product/review/review_view.sky?intReviewSeq=" + intReviewSeq + "&dispNum=0104&targetPage=" + targetPage.value + "&targetGroup=" + targetGroup.value;
		
		location.href = url;
	}

	function List(){
		var dispNum = $('dispNum');
		var telCode = $('telCode');
		var seq = $('seq');

		var url = "/product/product.sky?seq=" + seq.value + "&dispNum=" + dispNum.value + "&telCode=" + telCode.value;
		
		location.href = url;
	}
