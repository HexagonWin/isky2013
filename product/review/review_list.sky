	function goList(targetPage, targetGroup) {

		var obj = document.reviewPage;

		obj.targetPage.value = targetPage;
		obj.targetGroup.value = targetGroup;
		obj.action = "/product/review/review_list.sky";
		obj.submit();
	}

	function detail(intReviewSeq){
		
		var targetPage = $('targetPage');
		var targetGroup = $('targetGroup');
		var strTelecomCode = $('strTelecomCode');
		var intSeriesSeq = $('intSeriesSeq');
		var intPrdSeq = $('intPrdSeq');
		var keyword = $('keyword');
		
		var url = "/product/review/review_view.sky?intReviewSeq=" + intReviewSeq + "&dispNum=&targetPage=" + targetPage.value + "&targetGroup=" + targetGroup.value + "&strTelecomCode=" + strTelecomCode.value + "&intSeriesSeq=" + intSeriesSeq.value + "&intPrdSeq=" + intPrdSeq.value + "&keyword=" + keyword.value;
		
		parent.location.href = url;
	}

	function List(){
		var form = $('reviewPage');
		form.action = "/product/review/review_list.sky";
		form.method = "post";
		form.submit();
	}
