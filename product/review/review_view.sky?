<script language="javascript">

	var badUser = "";
	
	function List(){

		var targetPage = $('targetPage');
		var targetGroup = $('targetGroup');

		location.href = "/product/review/review.sky?dispNum=&targetPage=" + targetPage.value + "&targetGroup=" + targetGroup.value;
	}

	function Write(){
		var isBadVal = badUserChk(badUser);
		
		if(isBadVal == false){
			
			location.href = "/product/review/review_write.sky?dispNum=";
		}
	}

	function Vote(){
		var ans = confirm("추천하시겠습니까?\n\n리뷰당 한번씩만 가능합니다.");
		
		if(ans == true){
			var form = $('reviewPage');
			form.method = "post";
			form.action = "/product/review/review_vote.sky";
			form.submit();
		}else{
			return;
		}
	}

	function Modify(){
		var form = $('reviewPage');
		form.method = "post";
		form.action = "/product/review/review_modify.sky";
		form.submit();
	}

	function Delete(){
		var ans = confirm("삭제하시겠습니까?");

		if(ans == true){
			var form = $('reviewPage');
			form.method = "post";
			form.action = "/product/review/review_delete.sky";
			form.submit();
		}else{
			return;
		}
	}

	function NotDelete(){
		alert("주간 베스트로 선정된 게시물은 삭제하실 수 없습니다.");
		return;
	}

	//아이 프레임 리사이즈
	function resizeFrame(iframeObj){
		var innerBody = iframeObj.contentWindow.document.body;

		var innerHeight = innerBody.scrollHeight + (innerBody.offsetHeight - innerBody.clientHeight);
		var innerWidth = innerBody.scrollWidth + (innerBody.offsetWidth - innerBody.clientWidth);

		iframeObj.style.height = innerHeight;
		iframeObj.style.width = innerWidth;
	}
		
</script>
