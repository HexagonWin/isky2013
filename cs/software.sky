<script>
	var dispNum="050202";
	var firstDepth ="05";

		window.onload = function() {

			var telecomName = "";
			var seriesName = "";
			var prdName = "";
			var intSeriesSeq = "0";
			var intPrdSeq = "0";


			if(document.getElementById("searchPrdName").value != ""){
				prdName = document.getElementById("searchPrdName").value;
				document.getElementById("searchPrdName").style.backgroundImage = 'none';
			}

			var tn = document.getElementById("telecomList");
			var sn = document.getElementById("series");
			var mn = document.getElementById("model");

			if(telecomName != ''){

				for ( var i = 0 ; i < tn.length ; i++ ) {
					if(tn[i].value == telecomName){
						tn[i].selected = true;
					}
				}
			}

			if(seriesName != ''){
				while (sn.childNodes.length > 0) {
					sn.removeChild(sn.childNodes[0]);
				}
				option = document.createElement("option");
				option.value =seriesName;
				option.appendChild(document.createTextNode(seriesName));
				sn.appendChild(option);
			}

			if(prdName != ''){

				while (mn.childNodes.length > 0) {
					mn.removeChild(mn.childNodes[0]);
				}

				option = document.createElement("option");
				option.value =prdName;
				option.appendChild(document.createTextNode(prdName));
				mn.appendChild(option);
			}

			if(prdName == ''){
				list.goFrameList(telecomName,seriesName,prdName,intSeriesSeq,intPrdSeq);
			}else{
				list.goFrameListPrd(prdName);
			}

		}
</script>
