<script language="javascript">
	function goList(targetPage, targetGroup) {
		
		var obj = document.eventPage;
	
		obj.targetPage.value = targetPage;
		obj.targetGroup.value = targetGroup;
		obj.action = "/etc/event_view_list.sky";
		obj.submit();
	}
	
	function Detail(intSeq, rnum, strUrl){
		
		var targetPage = $('targetPage');
		var targetGroup = $('targetGroup');

		var url = "";

		if(strUrl != "" && strUrl != "null"){
			url = strUrl;
		}else{
			url = "/etc/event_view.sky?intSeq=" + intSeq + "&dispNum=&targetPage=1&targetGroup=1&rnum=" + rnum;
		}
		
		parent.location.href = url;
	}
	</script>
