<script type="text/javascript">

	/*tab*/
	function initTabMenu(tabContainerID) {
		var tabContainer = document.getElementById(tabContainerID);
		var tabAnchor = tabContainer.getElementsByTagName("a");
		var i = 0;
	
		for(i=0; i<tabAnchor.length; i++) {
			if (tabAnchor.item(i).className == "tab")
				thismenu = tabAnchor.item(i);
			else
				continue;
	
			thismenu.container = tabContainer;
			thismenu.targetEl = document.getElementById(tabAnchor.item(i).href.split("#")[1]);
			thismenu.targetEl.style.display = "none";
			thismenu.imgEl = thismenu.getElementsByTagName("img").item(0);
			thismenu.onclick = function tabMenuClick() {
				currentmenu = this.container.current;
				if (currentmenu == this)
					return false;
	
				if (currentmenu) {
					currentmenu.targetEl.style.display = "none";
					if (currentmenu.imgEl) {
						currentmenu.imgEl.src = currentmenu.imgEl.src.replace("_on.jpg", ".jpg");
					} else {
						currentmenu.className = currentmenu.className.replace("_on", "");
					}
				}
				this.targetEl.style.display = "";
				if (this.imgEl) {
					this.imgEl.src = this.imgEl.src.replace(".jpg", "_on.jpg");
				} else {
					this.className += " on";
				}
				this.container.current = this;
	
				return false;
			};
	
			if (!thismenu.container.first)
				thismenu.container.first = thismenu;
		}
		if (tabContainer.first)
			tabContainer.first.onclick();
	}

	/*png24*/
	function setPng24(obj) {
	    obj.width=obj.height=1;
	    obj.className=obj.className.replace(/\bpng24\b/i,'');
	    obj.style.filter =
	    "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='"+ obj.src +"',sizingMethod='image');"
	    obj.src=''; 
	    return '';
	}

	// 이벤트 댓글 페이징 이동
	function goList(targetPage, targetGroup) {
	
		var obj = $('event');
	
		obj.targetPage.value = targetPage;
		obj.targetGroup.value = targetGroup;
		obj.action = "/etc/event/event_vegano5.sky";
		obj.submit();
	}

	// 이벤트 메인으로 이동
	function List() {
		location.href = "/etc/event.sky?dispNum=";
	}

	function goInputReplay() {
		var form1 = $('event');
	    
		var strTxt = "blog:" + form1.strCommentTxt1.value + "|VegaNo:" + form1.strCommentTxt2.value + "|text:" + form1.strCommentTxt3.value;
	    var loginCheck = form1.logsess.value;
	    
	    if(loginCheck == "N"){
	    	LoginConfirm('http://www.isky.co.kr/etc/event/event_vegano5.sky?');
	    	return;
	    }
	    
		if(form1.strCommentTxt1.value == "" || form1.strCommentTxt1.value== "http://") {
			alert("블로그 주소를 입력해주세요");
			return;
		} else {
			if(form1.strCommentTxt1.value.indexOf("http://", "0") != 0) {
				alert("잘못된 입력 형식입니다.");
				return;
			}
			
			if(form1.strCommentTxt2.value == "") {
                alert("본인의 Vega No.를 선택 해 주세요.");
                return;
            }
			
			if(form1.strCommentTxt3.value == "" || form1.strCommentTxt3.value == "체험단에 임하는포부를 250자 이내로 작성해 주세요.") {
                alert("체험단에 임하는포부를 250자 이내로 작성해 주세요.");
                return;
            }
			
			
			if(form1.strCommentTxt3.value.length > "250"){
				alert("체험단에 임하는포부를 250자 이내로 작성해 주세요.");
				form1.strCommentTxt3.value = form1.strCommentTxt3.value.substring(0, 250);
                return;
			}


			var strResult = "";
	
			strResult = strTxt;
			
			form1.strComment.value = strTxt;
			form1.strResult.value = strResult;
			
			form1.action = "event_vegano5_insert.sky";
			form1.target = "dataProcess";
			form1.method = "post";
			form1.submit();
		}
	
	}

	
	// 로그인 체크 로직
	function LoginChk() {
		var errorMsg = $('alertMsg').value;
	    
		if(errorMsg == "NotLogin")
			LoginConfirm('http://www.isky.co.kr/etc/event/event_vegano5.sky?');
    }
 

	function copyClip() {

	} 
	
	function clearDefaultValue(obj){
		if(obj.name == "strCommentTxt2"){
            if(obj.value=="본인의 Vega No.를 선택 해 주세요.")
            obj.value = "";
            return;
        }
		
		if(obj.name == "strCommentTxt3"){
            if(obj.value=="체험단에 임하는포부를 250자 이내로 작성해 주세요.")
            obj.value = "";
            return;
        }
	}
	
	
