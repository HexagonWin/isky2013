<script language="javascript">
	function InputChk(){
		var form = document.event;

		if(form.intEventSeq.value == "" || form.intEventSeq.value == "null"){
			
			alert("이벤트정보가 맞지 않습니다.");
			location.href = "/etc/event.sky";
			return;
		}
		
		var chk = false;

		for(var i=0; i < form.radio.length; i++){
			if(form.radio[i].checked == true){
				chk = true;
			}
		}

		if(chk == false){
			alert("정답을 선택해 주세요");
			return;
		}else{
			if(form.radio[1].checked != true){
				alert("정답이 아닙니다. 다시 선택해 주세요");
				return;
			}
		}
		
		form.target = "dataProcess";
		form.action = "/etc/event_entry_insert.sky";
		form.submit();
	
	}
	
	function Trim(string){
	
	    for(;string.indexOf(" ")!= -1;){
	     	string = string.replace(" ","");
	    }
	 
	 	return string;
	}
	
	function LoginChk(){
		LoginPop("/etc/event_450.sky");
		return;
	}	
</script>
