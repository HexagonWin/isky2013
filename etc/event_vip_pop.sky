<script language="javascript">
	function InputChk(){
		var form = document.event;

		if(form.strContent.value == ""){
			alert('정답을 입력해 주세요');
			form.strContent.focus();
			return;
		}

		var strContent = Trim(form.strContent.value);
		
		if(strContent == "오마주" || strContent.toLowerCase() == "hommage"){

			form.target = "dataProcess";
			form.action = "/etc/event_vip_insert.sky";
			form.submit();

		}else{
			alert("정답이 아닙니다. 다시 입력해 주세요");
			form.strContent.value = "";
			form.strContent.focus();
			return;
		}
		
	}

	function Trim(string){

	    for(;string.indexOf(" ")!= -1;){
	     	string = string.replace(" ","");
	    }
	 
	 	return string;
	}

	function LoginChk(){
		LoginPop("/etc/event_vip_pop.sky");
		return;
	}	
</script>
