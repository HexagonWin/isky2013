<script language="javascript">
	function goList(targetPage, targetGroup) {
	
		var obj = document.noticePage;
	
		obj.targetPage.value = targetPage;
		obj.targetGroup.value = targetGroup;
		obj.action = "/etc/notice_view_list.sky";
		obj.submit();
	}

	function Detail(intSeq){
		
		var targetPage = $('targetPage');
		var targetGroup = $('targetGroup');

		var url = "/etc/notice_view.sky?intSeq=" + intSeq + "&dispNum=&targetPage=1&targetGroup=1";
		
		parent.location.href = url;
	}
</script>
